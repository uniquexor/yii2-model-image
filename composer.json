{
    "name": "unique/yii2-model-image",
    "description": "Allows to easily attach an image to a model in Yii2 framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","i18n","translation","urlmanager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Algirdas GureviÄius",
            "email": "a.gurevicius@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-imagine": "~2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "unique\\yii2modelimage\\": "src/",
            "unique\\yii2modelimagetest\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
